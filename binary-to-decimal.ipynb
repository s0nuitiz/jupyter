   "source": [
    "n = int(input(\"Enter a Binary number : \"))\n",
    "d = 0\n",
    "i = 0\n",
    "b = n\n",
    "while b > 0:\n",
    "    rem = b % 10\n",
    "    d += rem*(2**i)\n",
    "    b = b//10\n",
    "    i += 1\n",
    "print(\"Dacimal of\", n, \"is\", d)"
   ]
